package com.company;
import java.util.Scanner;

public class Divisible {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        System.out.println("Enter the number: ");
        int n = s.nextInt();

        if((n%5==0) && (n%11==0))
        {
            System.out.println("Number "+ n +"\tis divisible by 5 and 11");
        }
        else
        {
            System.out.println("Number "+ n + "\tis not divisible by 5 and 11");
        }
    }
}
