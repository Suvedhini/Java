package com.company;
import java.util.Scanner;

public class Factorial {
    public static void main(String[] args) {
        int sum = 0;
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter the number: ");
        int n = sc.nextInt();

        for (int i=n;i>=1;i--)
        {
            n = n * (i - 1);
            System.out.println(n);


        }



    }

}
