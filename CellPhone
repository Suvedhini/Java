package com.company;

class Phone{
    public void ring()
    {
        System.out.println("Ringing...");
    }
    public void vibrate()
    {
        System.out.println("Vibrating...");
    }
    public void call()
    {
        System.out.println("Calling...");
    }
}

public class CellPhone {
    public static void main(String[] args) {
        Phone ph = new Phone();
        ph.ring();
        ph.vibrate();
        ph.call();
    }
}
