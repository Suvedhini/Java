package com.company;

public class AssignmentOperator {
    public static void main(String[] args) {
        int a=5,b=10,c=20;
        a +=b;
        System.out.println(a);

        b -=c;
        System.out.println(b);

        b *=c;
        System.out.println(b);

        a /=c;
        System.out.println(a);

        a %=b;
        System.out.println(a);
    }
}
