package com.company;
import java.util.Scanner;

public class ReverseMultiplication {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        System.out.println("Enter the number: ");
        int n = s.nextInt();

        for (int i=10;i>=1;i--)
        {
            System.out.println(n + "*"+ i +"="+ n*i);
        }
    }
}
